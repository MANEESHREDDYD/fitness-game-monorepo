{
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "python.analysis.typeCheckingMode": "basic",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": true,
  "explorer.excludeGitignore": true,
  "explorer.autoReveal": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "files.exclude": {
    "**/.git": false,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": false,
    "**/.gradle": true,
    "**/build": true,
    "**/.idea": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.gradle": true,
    "**/build": true
  },
  "window.zoomLevel": 0,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.trimAutoWhitespace": true,
  "editor.wordWrap": "on",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "editor.rulers": [80, 120],
  "editor.renderWhitespace": "selection",
  "editor.renderControlCharacters": true,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "extensions.recommendations": [
    "ms-vscode.vscode-android",
    "msjsdiag.vscode-react-native",
    "rangav.vscode-thunder-client",
    "dsznajder.es7-react-js-snippets",
    "esbenp.prettier-vscode",
    "dbaeumer.vscode-eslint",
    "ms-python.python",
    "ms-python.vscode-pylance"
  ],
  "gradle.home": "${homedir}/.gradle",
  "android.showDeviceChooser": true,
  "debug.allowBreakpointsEverywhere": true,
  "debug.inlineValues": true,
  "debug.openDebug": "neverOpen",
  "react-native-tools.showLogcatOnStartup": true,
  "react-native-tools.logcatFilteringTextPattern": "FitnessGame|ReactNativeJS",
  "react-native-tools.androidSDKPath": "${homedir}\\AppData\\Local\\Android\\Sdk",
  "react-native-tools.nodeModulesMaxSearchDepth": 10,
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    }
  },
  "remote.extensionKind": {
    "ms-vscode.remote-server": "ui"
  }
}
