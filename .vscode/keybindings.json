[
  {
    "key": "ctrl+alt+a",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "emulator -avd Pixel_6_API_34\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+r",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "npm run android\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+d",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "npm run dev\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+t",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "Thunder Client: New Request\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+shift+m",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "npm test\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+shift+p",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "npm run android -- --mode=release\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+l",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "adb logcat | grep -i fitness\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+b",
    "command": "workbench.action.terminal.sendSequence",
    "args": { "text": "adb devices\n" },
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+alt+e",
    "command": "workbench.action.tasks.runTask",
    "args": "Android: Start Emulator + Deploy App"
  }
]
